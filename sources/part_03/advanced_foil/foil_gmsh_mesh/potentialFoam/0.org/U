FoamFile
{
    format      ascii;
    class       volVectorField;
    location    "0";
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

// *** Velocity magnitude is 60 m/s ***

// angle = 0 deg
internalField   uniform (60 0 0);

boundaryField
{
	inlet
	{
		type            freestreamVelocity;
        freestreamValue $internalField;
	}
	
	outlet
	{
		type            freestreamVelocity;
        freestreamValue $internalField;
	}

	top
	{
		type            symmetryPlane;
	}

	bottom
	{
		type            symmetryPlane;
	}

	foilup
	{
		type            noSlip;
	}

	foillow
	{
		type            noSlip;
	}

	defaultFaces
	{
		type            empty;
	}
}

// ************************************************************************* //
